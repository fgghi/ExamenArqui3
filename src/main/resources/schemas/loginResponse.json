{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "loginResponse",
    "description": "Login Response",
    "type": "object",
    "properties": {
        "token": {
            "description": "Login token",
            "type": "string"
        },
        "timestamp": {
            "description": "Login Timestamp",
            "type": "string",
            "format": "date-time"
        }
    },
    "required": ["token", "timestamp"]
}